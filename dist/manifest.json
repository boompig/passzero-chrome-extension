{
    "manifest_version": 2,
    "name": "PassZero",
    "version": "1.0.1",

    "description": "Chrome integration for PassZero",

    "author": "Daniel Kats",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "entry_search.html"
    },

    "permissions": [
        "activeTab",
        "storage",
        "cookies",
        "*://passzero.herokuapp.com/",
        "*://passzero.herokuapp.com/*"
    ]
}
